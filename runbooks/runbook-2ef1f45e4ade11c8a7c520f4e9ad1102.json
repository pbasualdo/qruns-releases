  "steps": [
    {
      "title": "Stop Service",
      "content": [
        {
          "type": "text",
          "text": "net stop MSSQLSERVER"
        }
      ]
    },
    {
      "title": "Start Service",
      "content": [
        {
          "type": "text",
          "text": "net start MSSQLSERVER"
