  "title": "[IAAS] SQL Log File Full",
  "category": "Database",
  "type": "qrun",
  "shortDescription": "Transaction log is full (Error 9002).",
  "fullDescription": "Log drive is full or log file reached max size. Check log_reuse_wait_desc.",
  "tags": [
    "SQL",
    "Storage",
    "Log"
  ],
  "steps": [
    {
      "title": "Check Reuse Wait",
      "content": [
        {
          "type": "code",
          "language": "sql",
          "code": "SELECT name, log_reuse_wait_desc FROM sys.databases"
        }
      ]
    },
    {
      "title": "Backup Log",
      "content": [
        {
          "type": "code",
          "language": "sql",
          "code": "BACKUP LOG [DBName] TO DISK='NUL' -- If non-prod"
        }
      ]
    }
  ],
  "service": "IAAS"
}